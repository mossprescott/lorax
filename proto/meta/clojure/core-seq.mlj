              (make-node :kernel/nil)
  
              :else
              (make-node :core/cons {
                :first
                (make-node :kernel/var {
                  :ref (ref-node :i_range)
                })
                
                :rest
                (make-node :kernel/app {
                  :expr
                  (make-node :kernel/var {
                    :ref (ref-node :r_p_range)
                  })

                  :args
                  (make-node :kernel/args [
                    (make-node :core/plus {
                      :left
                      (make-node :kernel/var {
                        :ref (ref-node :i_range)
                      })
                      
                      :right
                      (make-node :kernel/int { :value 1 })
                    })
                  ])
                })
              })
            })
          })
      
          :body
          (make-node :kernel/app {
            :expr
            (make-node :kernel/var {
              :ref (ref-node :r_range)
            })
        
            :args
            (make-node :kernel/args [
              (make-node :kernel/unquote {
                :body
                (make-node :kernel/var {
                 :ref
                 (ref-node :range_min) 
                })
              })
            ])
          })
        })
      })
    })
  })
  
