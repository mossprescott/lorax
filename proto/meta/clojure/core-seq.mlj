    ; let end = [max] in let r = fn r [i] if i > end then nil else lazy-cons(i, r(i+1)))
    ; in r(min)
    :expand
    (make-node :kernel/quote {
      :body
      (make-node :kernel/let {
        :bind
        (make-node :kernel/bind :end_range {})
      
        :expr
        (make-node :kernel/unquote {
          :body
          (make-node :kernel/var {
           :ref
           (ref-node :range_max) 
          })
        })
      
        :body
        (make-node :kernel/let {
          :bind
          (make-node :kernel/bind :r_range {})
      
          :expr
          (make-node :kernel/lambda :r_p_range {
            :params 
            (make-node :kernel/params [
              (node :kernel/bind :lorax/id :i_range)
            ])
          
            :body
            (make-node :kernel/if {
              :test
              (make-node :core/greater {
                :left
                (make-node :kernel/var {
                  :ref (ref-node :i_range)
                })
                  
                :right
                (make-node :kernel/var {
                  :ref (ref-node :end_range)
                })
              })
            
